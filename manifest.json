{
  "manifest_version": 3,
  "name": "Audyteko SDR",
  "version": "1.0",
  "description": "Automatyzacja kontaktu z agencjami na Otodom.",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://www.otodom.pl/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
